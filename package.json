    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "@vitejs/plugin-vue": "^1",
    "@vue/compiler-sfc": "^3",
    "@vue/eslint-config-prettier": "^6",
    "@vue/eslint-config-typescript": "^7",
    "@vuedx/typecheck": "^0",
    "@vuedx/typescript-plugin-vue": "^0",
    "autoprefixer": "^10",
    "eslint": "^7",
    "eslint-config-prettier": "^8",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-vue": "^7",
    "husky": "^4",
    "lint-staged": "^10",
    "postcss-calc": "^8",
    "postcss-import": "^14",
