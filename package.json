{
  "name": "vuetube-front",
  "version": "2.0.0",
  "description": "Youtube Client clone",
  "keywords": [
    "youtube",
    "es6",
    "typescript",
    "vite",
    "esbuild"
  ],
  "author": "RaphaÃ«l Morineau <raphael.morineau@gmail.com>",
  "license": "MIT",
  "private": true,
  "repository": "git@github.com:Sharlaan/Vuetube-front.git",
